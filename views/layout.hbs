    <link rel='stylesheet' href='/stylesheets/tobi.min.css' />
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js"></script>
    <script>
      WebFont.load({
        google: {
          families: ['Lato', 'Roboto Condensed', 'Material Icons']
        },
        timeout: 10000
      });
    </script>
  </head>
  <body class="body-class">
    <div id="loading" class="loading loading--in">
      <div class="lds-ring"><div></div><div></div><div></div><div></div></div>
    </div>
    <div class="demo-layout-transparent mdl-layout mdl-js-layout">
      <div class="nav__mobile-logo">
        <img src="/images/logo.png">
      </div>

      <header style="pointer-events: auto" class="mdl-layout__header mdl-layout__header--transparent">
        <div class="mdl-layout__header-row">
          <div id="search-results" class="search-results">
          </div>
          <!-- Title -->
          <a href="/" id="header-logo" class="nav__logo-container">
            <img src="/images/logo.png">
          </a>
          <!-- Add spacer, to align navigation to the right -->
          <div class="mdl-layout-spacer"></div>
          <!-- Navigation -->
          <nav class="mdl-navigation">
            <a id="header-about" class="mdl-navigation__link" href="">ABOUT</a>
            <a id="header-login" class="mdl-navigation__link" href="http://www.globaltourcreatives.com/gtcforce/log-in/" target="_blank">LOGIN</a>
            <a id="header-contact" class="mdl-navigation__link" href="">CONTACT</a>
            <div class="mdl-textfield mdl-js-textfield mdl-textfield--expandable
                        mdl-textfield--floating-label mdl-textfield--align-right">
              <label class="mdl-button mdl-js-button mdl-button--icon"
                     for="fixed-header-drawer-exp">
                <i class="material-icons">search</i>
              </label>
              <div class="mdl-textfield__expandable-holder">
                <input class="mdl-textfield__input" type="text" name="sample"
                       id="fixed-header-drawer-exp">
              </div>
            </div>
          </nav>
        </div>
      </header>
      <div class="mdl-layout__drawer">
        <nav class="mdl-navigation">
          <a id="drawer-home" class="mdl-navigation__link" href="">HOME</a>
          <a id="drawer-about" class="mdl-navigation__link" href="">ABOUT</a>
          <a id="drawer-login" class="mdl-navigation__link" href="http://www.globaltourcreatives.com/gtcforce/log-in/" target="_blank">LOGIN</a>
          <a id="drawer-contact" class="mdl-navigation__link" href="">CONTACT</a>
        </nav>
      </div>
      <main class="mdl-layout__content">
      </main>
    </div>
    <div class="body-container">
      {{{body}}}
      {{#if showtour}}
        {{#each tvSpots}}
          <div id="lightbox-video-1">
            <video controls
              src="{{{this.spotURL}}}"
              width="620">
              Sorry, your browser doesn't support embedded video
            </video>
          </div>
        {{/each}}
        {{#each radioSpots}}
          <div id="lightbox-audio-1">
            <audio controls
              src="{{{this.spotURL}}}">
              Sorry, your browser doesn't support the audio element
            </audio>
          </div>
        {{/each}}
      <div class="details details--open details--open-init">
        <div class="details__bg details__bg--up"></div>
        <div class="details__bg details__bg--down"></div>
        <img class="details__img" src="{{mainImage}}" alt="img 01"/>
        <h3 class="details__subtitle">{{subTitle}}</h3>
        <h2 class="details__title">{{tour}}</h2>
        <div class="details__deco"></div>
        <div
          class="details__assets-container">
          <div
            {{#unless radioSpots}}style="display:none;"{{/unless}}
            class="details__assets-col details__assets-audio">
            <div
              class="details__asset-icon details__audio-icon">
              <img src="/images/asset-audio.png" />
              <span>RADIO</span>
            </div>
            <div
              class="details__asset details__audio-asset">
              <a
                data-lightboxindex="0"
                href="#lightbox-audio-1"
                data-href="#lightbox-audio-1"
                data-type="html"
                class="lightbox lightbox-audio">
                <i class="material-icons">mic</i><b>Generic</b>
              </a>
            </div>
          </div>
          <div
            {{#unless tvSpots}}style="display:none;"{{/unless}}
            class="details__assets-col details__assets-video">
            <div
              class="details__asset-icon details__audio-icon">
              <img src="/images/asset-video.png" />
              <span>TV</span>
            </div>
            <div
              class="details__asset details__audio-asset">
              <a
                data-lightboxindex="1"
                href="#lightbox-video-1"
                data-href="#lightbox-video-1"
                data-type="html"
                class="lightbox lightbox-video">
                <i class="material-icons">play_circle_outline</i><b>Generic</b>
              </a>
            </div>
          </div>
          <div
            {{#unless printItems}}style="display:none;"{{/unless}}
            class="details__assets-col details__assets-photo">
            <div
              class="details__asset-icon details__audio-icon">
              <img src="/images/asset-image.png" />
              <span>ART</span>
            </div>
            <div
              class="details__asset details__audio-asset">
                <a
                  data-lightboxindex="2"
                  href="{{#if printItems}}{{#each printItems}}{{this.thumbnailURL}}{{/each}}{{/if}}"
                  class="lightbox lightbox-print lightbox-image">
                  <i class="material-icons">create</i><b>Generic</b>
                </a>
            </div>
          </div>
        </div>
        <button class="details__close"><svg class="icon icon--cross"><use xlink:href="#icon-cross"></use></svg></button>
      </div>
      {{/if}}
    </div>
  </body>
  <script src="https://cdn.polyfill.io/v2/polyfill.min.js"></script>
  <script src="https://code.getmdl.io/1.3.0/material.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.17.10/lodash.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/animejs/2.2.0/anime.min.js"></script>
  <script src="/javascripts/tobi.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/vanilla-lazyload@11.0.5/dist/lazyload.min.js"></script>
  <script>
    // define dependencies
    depp.define({
      'animate': ['https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.0/animate.min.css'],
      'parallax': ['/javascripts/parallax.js'],
      'aos': ['https://unpkg.com/aos@2.3.1/dist/aos.js', 'https://unpkg.com/aos@2.3.1/dist/aos.css'],
      // 'plugin2': ['#jquery', '/path/to/plugin2.js', '/path/to/plugin2.css', '/path/to/plugin2.png']
    });
    var deferAnimate = () => {
      depp.require(['animate'], function() {
        animationsAvailable = true;
      });
    };
    var deferParallax = () => {
      depp.require(['parallax'], function() {
        // do something after parallax loads
      });
    };
    var deferAOS = () => {
      depp.require(['aos'], function() {
        if (
          typeof loadingHandler !== 'undefined'
          && loadingHandler.DOM.about[0].style.display !== 'none'
        ) {
          loadingHandler.initAOS = true;
          AOS.init();
        }
      });
    };
  </script>
  <script defer src="/javascripts/navigation.js"></script>
  <script defer src="/javascripts/card-grid.js"></script>
  <script defer src="/javascripts/search.js"></script>
  <script defer src="/javascripts/index.js"></script>
  {{#if showtour}}
  <script> if (!tobi) { var tobi = new Tobi() }</script>
  {{/if}}
  <script>
    var lazyLoadInstance = new LazyLoad({
        elements_selector: '[data-src]'
        // ... more custom settings?
    });
  </script>
</html>
